{
  "name": "xsd-schema-validator",
  "version": "0.9.0",
  "description": "A (XSD) schema validator for nodejs",
  "main": "lib/validator.js",
  "scripts": {
    "all": "run-s lint check-types test",
    "check-types": "tsc --noEmit",
