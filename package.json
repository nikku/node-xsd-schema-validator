  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "^20.17.6",
    "chai": "^4.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-bpmn-io": "^1.0.0",
    "mocha": "^10.8.2",
    "npm-run-all2": "^7.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "which": "^5.0.0"
  },
  "files": [
    "lib",
    "support"
